<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<project name="Minijax">
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.5</version>
    </skin>
    <custom>
        <fluidoSkin>
            <topBarEnabled>false</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
            <leftColumnClass>span3</leftColumnClass>
            <bodyColumnClass>span9</bodyColumnClass>
            <gitHub>
                <projectId>minijax/minijax</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>black</ribbonColor>
            </gitHub>
        </fluidoSkin>
    </custom>
     <bannerLeft>
        <name>Minijax</name>
        <href>https://minijax.org/</href>
    </bannerLeft>
    <body>
        <menu name="Overview">
            <item name="Introduction" href="https://minijax.org/index.html"/>
            <item name="Motivation" href="https://minijax.org/motivation.html"/>
            <item name="Documentation" href="https://minijax.org/docs.html"/>
            <item name="License" href="https://minijax.org/license.html"/>
        </menu>
        <menu name="Examples">
            <item name="Hello World" href="https://minijax.org/minijax-examples/minijax-example-hello/index.html"/>
            <item name="JSON" href="https://minijax.org/minijax-examples/minijax-example-json/index.html"/>
            <item name="Websockets" href="https://minijax.org/minijax-examples/minijax-example-websocket/index.html"/>
            <item name="HTTPS" href="https://minijax.org/minijax-examples/minijax-example-ssl/index.html"/>
            <item name="Swagger" href="https://minijax.org/minijax-examples/minijax-example-swagger/index.html"/>
            <item name="Todo Backend" href="https://minijax.org/minijax-examples/minijax-example-todo-backend/index.html"/>
            <item name="Pet Clinic" href="https://minijax.org/minijax-examples/minijax-example-petclinic/index.html"/>
            <item name="Minitwit" href="https://minijax.org/minijax-examples/minijax-example-minitwit/index.html"/>
        </menu>
        <menu ref="parent"/>
        <menu ref="reports"/>
        <menu ref="modules"/>
    </body>
</project>