{{<master}}
{{$title}}Edit Owner{{/title}}
{{$content}}
<h2>Edit Owner</h2>
<form method="POST">
  <div class="form-group">
    <label for="name">Name</label>
    <input id="name" name="name" class="form-control" type="text" placeholder="Name" value="{{owner.name}}"  required>
  </div>
  <div class="form-group">
    <label for="address">Address</label>
    <input id="address" name="address" class="form-control" type="text" placeholder="Address" value="{{owner.address}}"  required>
  </div>
  <div class="form-group">
    <label for="city">City</label>
    <input id="city" name="city" class="form-control" type="text" placeholder="City" value="{{owner.city}}"  required>
  </div>
  <div class="form-group">
    <label for="telephone">Telephone</label>
    <input id="telephone" name="telephone" class="form-control" type="tel" placeholder="Telephone" value="{{owner.telephone}}"  required>
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
{{/content}}
{{/master}}